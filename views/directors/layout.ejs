                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" title="Display / Hide Side Menu"><i
                            class="fas fa-bars"></i></a>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->

        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="https://wwsu1069.org" class="brand-link">
                <img src="/images/display/logo_true.png" alt="WWSU Logo"
                    class="brand-image img-circle bg-white elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">WWSU 106.9 FM</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu"
