    <script src="/plugins/jdenticon/js/jdenticon.min.js"></script>
    <!--OneSignal-->
    <script
      src="https://cdn.onesignal.com/sdks/OneSignalSDK.js"
      async=""
    ></script>

    <!-- Summernote -->
    <script src="/plugins/summernote/summernote.min.js"></script>
