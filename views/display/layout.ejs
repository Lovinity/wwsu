        #nowplaying-seek {
            width: 100%;
            height: 100%;
            position: absolute;
            margin: 0px, 20px, -56px, 20px;
        }

        #nowplaying {
            transition: color 0.5s ease-in-out, background-color 0.5s ease-in-out, opacity 0.5s linear;
        }

        #nowplaying-line1 {
            overflow: hidden;
        }

        #nowplaying-line2 {
            overflow: hidden;
        }

        #dj-alert {
            z-index: 1010;
