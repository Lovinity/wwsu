            height: 6em;
            top: 9em;
            left: 14.25em;
            z-index: 30;
        }

        #clock #minute {
            border-radius: 0.5em;
            width: 0.5em;
            height: 11em;
            top: 4em;
            left: 14.5em;
            z-index: 20;
        }

        #clock #second {
            border-radius: 0.25em;
            width: 0.25em;
            height: 14em;
            top: 1em;
            left: 14.75em;
            z-index: 10;
            background-color: red;
        }

        #clock #center {
            height: 1em;
            width: 1em;
            border-radius: 50%;
            top: 14.25em;
            left: 14.25em;
            z-index: 50;
            transform: rotate(0deg);
        }

        .smallTick {
