    <link rel="stylesheet" href="../../plugins/iziToast/css/iziToast.min.css">
    <link rel="stylesheet" href="../../plugins/material/css/material.min.css">
    <link rel="stylesheet" href="../../plugins/animate/css/animate.css">
    <link rel="stylesheet" href="../../plugins/quill/css/quill.display.css">
    <link rel="stylesheet" href="../../plugins/fontawesome-free/css/fontawesome.min.css" type="text/css">
