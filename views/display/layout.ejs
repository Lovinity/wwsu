    <!-- Clock styling -->
    <style>
      .clock-container {
        width: 200px;
        height: 200px;
        position: absolute;
        left: 50%;
        margin-left: -100px;
        top: 50%;
        margin-top: -100px;
      }

      .clock {
        width: 100%;
        height: 100%;
        margin: auto;
        position: relative;
      }

      .clock div,
      .faceBox {
        border: 0;
        background-color: black;
        position: absolute;
        transform-origin: center bottom;
        -webkit-transform-origin: center bottom;
        -moz-transform-origin: center bottom;
        -o-transform-origin: center bottom;
        transform: rotate(0deg);
      }

      .dark-mode .clock div,
      .dark-mode .faceBox {
        background-color: white;
      }

      .clock .clock-hour {
        border-radius: 0.5em 0.5em 0 0;
        width: 3%;
        height: 20%;
        top: 30%;
        left: 47.5%;
        z-index: 30;
      }

      .clock .clock-minute {
        border-radius: 0.33em;
        width: 2%;
        height: 36.66%;
        top: 13.33%;
        left: 48.33%;
        z-index: 20;
      }

      .clock .clock-second {
        border-radius: 0.2em;
        width: 0.83%;
        height: 46.66%;
        top: 3.33%;
        left: 49.16%;
        z-index: 10;
        background-color: red;
      }

      .clock .clock-center {
        height: 6.66%;
        width: 6.66%;
        border-radius: 50%;
        top: 46.66%;
        left: 46.66%;
        z-index: 50;
        transform: rotate(0deg);
      }

      .smallTick {
        width: 3.33%;
        height: 16.66%;
        background-color: black;
      }

      .dark-mode .smallTick {
        background-color: white;
      }

      .mediumTick {
        width: 8.33%;
        height: 26.66%;
        background-color: black;
      }

      .dark-mode .mediumTick {
        background-color: white;
      }

      .largeTick {
        width: 13.33%;
        height: 26.66%;
        background-color: black;
        position: relative;
      }

      .dark-mode .largeTick {
        background-color: white;
      }

      .tickNum {
        font-family: Arial, sans-serif;
        font-size: 0.75em;
        line-height: 0;
        color: black;
        position: relative;
        top: 3.33%;
        left: -0.66%;
        z-index: 1;
      }

      .dark-mode .tickNum {
        color: white;
      }

      .faceBox {
        border: 0;
        width: 0;
        height: 50%;
        top: 0;
        left: 50%;
        position: absolute;
        text-align: center;
        overflow: visible;
        margin: 0 auto;
      }
    </style>

    <!-- Pace styling -->
    <style>
      .pace {
        -webkit-pointer-events: none;
        pointer-events: none;

        -webkit-user-select: none;
        -moz-user-select: none;
        user-select: none;
      }

      .pace-inactive {
        display: none;
      }

      .pace .pace-progress {
        background: #29d;
        position: fixed;
        z-index: 2000;
        top: 0;
        right: 100%;
        width: 100%;
        height: 2px;
      }
    </style>

    <!-- Slides styling -->
    <style>
      .scale-wrapper {
        position: absolute;
        left: 0;
        top: 0;
        right: 0;
        bottom: 0;
      }

      .scale-content {
        -ms-transform-origin: top left;
        -webkit-transform-origin: top left;
        transform-origin: top left;
        position: absolute;
      }
    </style>

