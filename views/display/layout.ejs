#nowplaying-seek {
  width: 100%;
  height: 100%;
  position: absolute;
  margin: 0px, 20px, -56px, 20px;
}
#nowplaying {
    transition: color 0.5s ease-in-out, background-color 0.5s ease-in-out, opacity 0.5s linear;
}
#nowplaying-line1 {
    overflow: hidden;
}
#nowplaying-line2 {
    overflow: hidden;
}
#alert-marquee {
    overflow: hidden;
}
            .loader2 {
                border: 16px solid #455A64; /* Light grey */
                border-top: 16px solid #F44336; /* Blue */
                border-radius: 50%;
                width: 50%;
                height: 50%;
                animation: spin 2s linear infinite;
            }
            @keyframes spin {
                0% { transform: rotate(0deg); }
                100% { transform: rotate(360deg); }
            }
            .overlay {
                position: fixed;
                display: block;
                width: 100%;
                height: 100%;
                top: 0;
                left: 0;
