        #eas-alert {
            z-index: 1011;
        }

        #alert-marquee {
            overflow: hidden;
        }

        .pulsate {
            animation: pulsate 2s ease-out;
            animation-iteration-count: infinite;
            opacity: 1;
        }

        @keyframes pulsate {
            0% {
                transform: scale(0.1, 0.1);
                opacity: 0.0;
            }

            50% {
                opacity: 1.0;
            }

            100% {
                transform: scale(1.2, 1.2);
                opacity: 0.0;
            }
        }

        .pulsate-alert {
            animation: alertPulse 2s ease-out;
            animation-iteration-count: infinite;
            opacity: 1;
        }

        @keyframes alertPulse {
            0% {
                opacity: 1;
            }

            50% {
                opacity: 0.5;
            }

            100% {
                opacity: 1;
            }
        }

        .loader2 {
            border: 16px solid #455A64;
            /* Light grey */
            border-top: 16px solid #F44336;
            /* Blue */
            border-radius: 50%;
            width: 50%;
            height: 50%;
            animation: spin 2s linear infinite;
        }

        @keyframes spin {
            0% {
                transform: rotate(0deg);
            }

            100% {
                transform: rotate(360deg);
            }
        }

