<meta name="Description" content="Tune in live to WWSU Radio and see what's currently playing. Chat with the DJ in our web chat box. And make track requests.">
        <style>
            body
            {
                overflow-x: hidden;
            }
            ::-webkit-scrollbar {
                width: 10px;
            }
            ::-webkit-scrollbar-track {
                background: #FFF;
                -webkit-box-shadow: inset 1px 1px 2px rgba(0,0,0,0.1);
            }
            ::-webkit-scrollbar-thumb {
                background: #CCC;
                -webkit-box-shadow: inset 1px 1px 2px rgba(0,0,0,0.2);
            }
            ::-webkit-scrollbar-thumb:hover {
                background: #AAA;
            }
            ::-webkit-scrollbar-thumb:active {
                background: #888;
                -webkit-box-shadow: inset 1px 1px 2px rgba(0,0,0,0.3);
            }
           .bs-callout {
                padding: 10px;
                margin: 10px 0;
                border: 0px solid #eee;
                border-left-width: 5px;
            }
            .bs-callout h4 {
                margin-top: 0;
                margin-bottom: 5px;
            }
            .bs-callout p:last-child {
                margin-bottom: 0;
            }
            .bs-callout code {
                border-radius: 3px;
            }
            .bs-callout+.bs-callout {
                margin-top: -5px;
            }
            .bs-callout-default {
                border-color: #9e9e9e;
                background: #383838;
            }
            .bs-callout-default h4 {
                color: #cfd8dc;
            }
            .bs-callout-trivial {
                border-color: #9e9e9e;
                background: #383838;
            }
            .bs-callout-trivial h4 {
                color: #cfd8dc;
            }
            .bs-callout-secondary {
                border-color: #9e9e9e;
                background: #383838;
            }
            .bs-callout-secondary h4 {
                color: #cfd8dc;
            }
            .bs-callout-primary {
                border-color: #e70d2f;
                background: rgba(115, 6, 23, 1);
            }
            .bs-callout-primary h4 {
                color: #faced5;
            }
            .bs-callout-success {
                border-color: #5cb85c;
                background: #1D441F;
            }
            .bs-callout-success h4 {
                color: #c8e6c9;
            }
            .bs-callout-danger {
                border-color: #f44336;
                background: #721818;
            }
            .bs-callout-danger h4 {
                color: #ffcdd2;
            }
            .bs-callout-warning {
                border-color: #ffc107;
                background:#4F3C03;
            }
            .bs-callout-warning h4 {
                color: #ffecb3;
            }
            .bs-callout-info {
                border-color: #2196f3;
                background: #0C3B69;
            }
            .bs-callout-info h4 {
                color: #bbdefb;
            }
            .bs-callout-urgent {
                border-color: #ff9800;
                background: #702700;
            }
            .bs-callout-urgent h4 {
                color: #ffe0b2;
            }

            .notification {
                position:absolute;
                top: 84px;
                right: 64px;
            }
        </style>
